  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc --rootDir app/ts --outdir app/js",
    "tsc:w": "tsc -w --rootDir app/ts --outdir app/js",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
